# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Application Environment
VITE_ENVIRONMENT=development
VITE_APP_NAME="Atelier Digital"
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_URL=https://api.atelier.digital
VITE_API_VERSION=v1

# Blockchain Configuration
VITE_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id
VITE_INFURA_PROJECT_ID=your_infura_project_id
VITE_ALCHEMY_API_KEY=your_alchemy_api_key

# Contract Addresses (Ethereum Mainnet)
VITE_NFT_CONTRACT_ADDRESS=0x...
VITE_MARKETPLACE_CONTRACT_ADDRESS=0x...

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Social Media
VITE_TWITTER_HANDLE=@AtelierDigital
VITE_DISCORD_INVITE=https://discord.gg/atelier-digital

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WALLET_CONNECT=true
VITE_ENABLE_MARKETPLACE=true
VITE_ENABLE_COMMENTS=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_MOCK_API=false